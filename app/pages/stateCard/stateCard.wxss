page{
	height: 100%;
	width: 100%;
}
.entity1{
	height: 100%;
	width: 100%;
	background-color: white;
	border-radius: 2%;
	display: flex;
	flex-direction: row;
	justify-content: center;
}

.inner{
	/**
	 * @author Luo'Xing'yue
	 * @createTime 2023-02-18
	 * 这个样式放在 .entity 中会导致右边框消失,怀疑和 map.wxss 中的样式有关
	*/
	height: 100%;
	width: 100%;
	border: 10rpx solid #d4d4d499;
	border-radius: 5%;
	padding: 20rpx;
	display: flex;
	flex-direction: row;
	justify-content: center;
}
.data_left{
	width: 100%;
	height: 100%;
	display: flex;
	flex-direction: column;
	text-align: center;
}
.data_right{
	
	width: 100%;
	height: 100%;
	margin-left: 10rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	text-align: center;
}
.custom_right{
	width: fit-content;
}
.default_right{
	width: fit-content;
}
.stationImage{
	width: 160rpx;
	height: 160rpx;
}
.dataImage{
	width: 160rpx;
	height: 160rpx;
	
}

.Description{
	text-align: center;
}