page{
	height: 100%;
}

.customCallout {
  /* box-sizing: border-box; */
  /* background-color: #000000; */
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.myMap{
	height: 100%;
	width: 100%;
}

.area{
	position: absolute;
	bottom: 0%;
	left: 1%;
	right: 1%;
	width: 100%;
	height: 5%;
}

.title{
	margin-top: 2%;
	margin-bottom: 2%;
	display: flex;
	flex-direction: column;
}

.more{
	background-color: #fffafae0;
	border-radius: 15%;
	border: solid #defaffff;
	position: absolute;
	bottom: 15%;
	left: 90%;
	/* #defaff00 */
}

.location{
	background-color: #fffafae0;
	border-radius: 15%;
	border: solid #defaffff;
	position: absolute;
	bottom: 7%;
	left: 90%;
	/* #defaff00 */
}

.map_cover{
	border-radius: 10%;
	background-color: aliceblue;
	height: 10%;
	position: absolute;
	bottom: 5%;
	left: 1%;
	right: 1%
}

