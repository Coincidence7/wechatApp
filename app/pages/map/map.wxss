page{
	height: 100%;
}

.customCallout {
  box-sizing: border-box;
  background-color: #fff; 
  border: 1px solid #ccc;
  border-radius: 30px;
  width: 150px;
  height: 40px;
  display: inline-flex;
  padding: 5px 20px;
  justify-content: center;
  align-items: center;
}

.myMap{
	height: 100%;
	width: 100%;
}

.area{
	position: absolute;
	bottom: 0%;
	left: 1%;
	right: 1%;
	width: 100%;
	height: 5%;
}

.title{
	margin-top: 2%;
	margin-bottom: 2%;
	display: flex;
	flex-direction: column;
}

.more{
	background-color: snow;
	border-radius: 15%;
	border: solid #defaffff;
	position: absolute;
	bottom: 20%;
	left: 88%;
	/* #defaff00 */
}

.location{
	background-color: snow;
	border-radius: 15%;
	border: solid #defaffff;
	position: absolute;
	bottom: 10%;
	left: 88%;
	/* #defaff00 */
}

.map_cover{
	border-radius: 10%;
	background-color: aliceblue;
	height: 10%;
	position: absolute;
	bottom: 5%;
	left: 1%;
	right: 1%
}
